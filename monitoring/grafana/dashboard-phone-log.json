{
  "title": "Phone Log Ingestion \u2014 API & Ingest",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Ingest rate (points/sec)",
      "id": 1,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(rate(app_ingested_points_total[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "type": "stat",
      "title": "Dropped rate (points/sec)",
      "id": 2,
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(rate(app_dropped_points_total[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "type": "stat",
      "title": "DB Failures (5m)",
      "id": 3,
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(increase(app_db_failures_total[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP request rate by status",
      "id": 4,
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (status) (rate(app_requests_total[5m]))",
          "legendFormat": "status {{status}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p50/p95/p99 (s)",
      "id": 5,
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(app_request_latency_seconds_bucket[5m])))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(app_request_latency_seconds_bucket[5m])))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(app_request_latency_seconds_bucket[5m])))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingested vs Dropped (rate)",
      "id": 6,
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(app_ingested_points_total[5m]))",
          "legendFormat": "ingested/s"
        },
        {
          "expr": "sum(rate(app_dropped_points_total[5m]))",
          "legendFormat": "dropped/s"
        }
      ]
    }
  ]
}